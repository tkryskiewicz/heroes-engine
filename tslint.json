{
  "extends": [
    "tslint:latest",
    "tslint-no-circular-imports",
    "tslint-react"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-submodule-imports": [
      true,
      "@storybook"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
